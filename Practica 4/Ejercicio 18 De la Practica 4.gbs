program {
    Sacar_Árboles(19)
}

procedure Poner_Semillas(cantidadDeSemillasAPoner) {
    /*PROPOSITO: Pone una cantidad dada de semillas.
    PRECONDICIONES: -
    OBSERVACIONES:
    *Cada semilla esta representada por una bolita roja.
    PARAMETROS:cantidadDeSemillasAPoner - Numero - cantidad de bolitas rojas a poner.
    */
    repeat (cantidadDeSemillasAPoner) {
    Poner(Rojo)
    }
}

