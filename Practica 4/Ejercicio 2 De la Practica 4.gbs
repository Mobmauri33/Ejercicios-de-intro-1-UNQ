program {
    /*PROPOSITO: invoca a un procedimiento que pone 6 bolitas de color Negro en la celda actual.
    PRECONDICION:-.
    */
    Poner6DeColor_()
}
procedure Poner6DeColor_(colorAPoner) {
    /*PROPOSITO: pone 6 bolitas de color Negro en la celda
actual.
    PRECONDICION:-.
    */
    repeat (6) {
        Poner(colorAPoner)
    }
}


