program {
    /*PROPOSITO: invoca a un procedimiento que mueve el cabezal tres posiciones al Oeste de la actual.
    PRECONDICION: Tener almenos 3 celdas hacia el oeste.
    */
    Mover3VecesAlOeste()
}
procedure Mover3VecesAlOeste() {
    /*PROPOSITO: mueve el cabezal tres posiciones al Oeste de
la actual.
    PRECONDICION: Tener almenos 3 celdas hacia el oeste.
    */
    repeat (3) {
        Mover(Oeste)
    }
}